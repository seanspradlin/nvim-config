{
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "lua",
    "css",
    "Markdown",
    "javascript",
    "json",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "tsx",
    "jsx",
    "terraform",
    "tf"
  ],
  "javascript.suggestionActions.enabled": false,
  "javascript.showUnused": false,
  "eslint.autoFixOnSave": true
}
